        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.java.hodipg.mapper.MemberMapper">
    <select id="selectLogin" resultType="com.java.hodipg.dto.MemberDto">
        select * from member where id=#{id} and pw=#{pw}
